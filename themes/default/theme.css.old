	color: #FFFFFF;
}
.f-nav a {
	text-shadow: none;
	color: #FFFFFF;
	font-size: 12px;
	font-family: "Funnel Display", sans-serif;
}
.f-post {
	margin-bottom: 8px;
}
.f-cat {
	background-image: url(images/f-cat.gif);
	background-repeat: repeat-x;
	background-position: top;
	margin: 0px;
	padding: 0px;
	height: 28px;
}
.f-cat td,  {
