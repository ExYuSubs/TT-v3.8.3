	padding-left: 18px;
}
.f-row:hover .f-img {
	background-image: url(images/alt2.png)
}
.alt1 {
	background-color: transparent;
	padding-left: 5px;
}
.alt2 {
	border: 1px solid #777;
	border-collapse: collapse;
